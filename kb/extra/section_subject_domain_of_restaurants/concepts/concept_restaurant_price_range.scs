            [Def. (Restaurant price range)]
            (*<- lang_en;;*);;
        <- definition;;
        <= nrel_sc_text_translation:
            ...
            (*
                -> rrel_example:
                    [Ценовой диапазон ресторана — условное обозначение средней цены за еду в ресторане.]
                    (*<- lang_ru;;*);;
            *);
