    [Ценовой диапазон ресторана]
    (*<- lang_ru;;*);
=> nrel_main_idtf:
    [Restaurant price range]
    (*<- lang_en;;*);

<- rrel_key_sc_element:
   ...
   (*
        => nrel_main_idtf:
            [Опр. (Ценовой диапазон ресторана)]
